<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.collections.FXCollections?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.Pane?>
<?import java.lang.String?>
<Pane prefHeight="132.0" prefWidth="500.0" GridPane.rowIndex="1" xmlns="http://javafx.com/javafx/8.0.112"
      xmlns:fx="http://javafx.com/fxml/1">
    <Button layoutX="14.0" layoutY="12.0" mnemonicParsing="false" onAction="#DoStep" text="Step"/>
    <ComboBox fx:id="sortselector" layoutX="336.0" layoutY="12.0" onAction="#SetSortString" prefWidth="150.0"
              promptText="BubbleSort">
        <items>
            <FXCollections fx:factory="observableArrayList">
                <String fx:value="BubbleSort"/>
                <String fx:value="InsertionSort"/>
                <String fx:value="QuickSort"/>
            </FXCollections>
        </items>
    </ComboBox>
    <Button fx:id="sortbutton" layoutX="16.0" layoutY="66.0" mnemonicParsing="false" onAction="#DoCompleteSort" text="sort"/>
</Pane>